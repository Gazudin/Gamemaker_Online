   draw_rectangle(x, y, x + (32 * image_xscale), y + (32 * image_yscale), 2);
}

// configure font
draw_set_font(fnt_login)
draw_set_valign(fa_center)

// draw text
if(string_length(text) &gt; 0 || focused){ 
    if(type = "obj_txt_Username"){
        scr_draw_border_text(x+8, y+sprite_height/2, text, c_white, c_black, 2, 1.0)
    } else if(type = "obj_txt_Password"){
        scr_draw_border_text(x+8, y+sprite_height/2, pwtext, c_white, c_black, 2, 1.0)
    }
}else{
    scr_draw_border_text(x+8, y+sprite_height/2, placeholder, c_white, c_black, 2, 1.0)
}

// reset font
draw_set_valign(fa_top)

